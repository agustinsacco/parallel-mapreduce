{
  "name": "parallel-mapreduce",
  "version": "1.0.2",
  "description": "Schedules long running iterations on parallel child processes",
  "main": "js/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json ./lib/{,**/}*.ts ./tests/{,**/}*.ts",
    "test": "mocha --timeout 10000 -r ts-node/register tests/**/*.test.ts",
    "coverage": "nyc npm test",
    "build": "tsc && cp ./lib/Worker.js ./js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.1.2",
    "@types/sinon": "^4.3.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "sinon": "^5.0.7",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "os": "^0.1.1",
    "path": "^0.12.7"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "lib/**/*.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html"
    ]
  }
}
